      "package": "com.andrew94.obdnativeapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "155b8dbb-cd0c-4887-a8d6-9fdeb5bc2874"
      }
